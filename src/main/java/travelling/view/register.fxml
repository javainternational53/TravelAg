      <TextField fx:id="fxUserName" layoutX="107.0" layoutY="285.0" promptText="UserName" />
      <TextField fx:id="fxEmail" layoutX="107.0" layoutY="412.0" promptText="Email" />
      <PasswordField fx:id="fxPassword" layoutX="107.0" layoutY="349.0" promptText="Password" />
      <Button fx:id="fxClear" layoutX="150.0" layoutY="455.0" mnemonicParsing="false" onAction="#ReseAll" text="Clear" textFill="#3196de" />
